@model Portal.Common.ViewModels.PharmacyViewModel
@using Portal.Common.Enums
<form method="post">
    <div class="row">
        <input asp-for="Pharmacy.PharmacyId" type="hidden" />
        <input asp-for="PharmacyStatus.PharmacyStatusId" type="hidden" />


        <div class="col-lg-12">
            <!--begin::Portlet-->
            <div class="kt-portlet kt-portlet--last kt-portlet--head-lg kt-portlet--responsive-mobile" id="kt_page_portlet">
                <div class="kt-portlet__head kt-portlet__head--lg">
                    <div class="kt-portlet__head-label">
                        <h3 class="kt-portlet__head-title">
                            Pharmacy Id:<small>@Model.Pharmacy.PharmacyId</small>
                        </h3>
                    </div>
                    <div class="kt-portlet__head-toolbar">
                        @*<a href="#" class="btn btn-clean kt-margin-r-10">
                            <i class="la la-pencil"></i>
                            <span class="kt-hidden-mobile">Edit</span>
                        </a>*@
                        @{
                            if (Model.User.ModulePermission == PermissionType.edit)
                            {
                                if (Model.ActionType == ActionType.Edit)
                                {
                                    <a href="/Pharmacy/list" class="btn btn-brand kt-margin-r-10">
                                        <i class="la la-remove"></i>
                                        <span class="kt-menu__link-text">Cancel</span>
                                    </a>
                                    <button asp-controller="Pharmacy" asp-action="Save" type="submit" class="btn btn-brand">
                                        <i class="la la-check"></i>
                                        <span class="kt-hidden-mobile">Save</span>
                                    </button>
                                }
                                else
                                {
                                    <button asp-controller="Pharmacy" asp-action="Edit" type="submit" class="btn btn-clean kt-margin-r-10">
                                        <i class="la la-pencil"></i>
                                        <span class="kt-hidden-mobile">Edit</span>
                                    </button>
                                }
                            }
                        }
                    </div>
                </div>
                <div class="kt-portlet__body">
                    <div class="kt-form kt-form--fit kt-form--label-right" id="kt_form">
                        <div class="row">
                            <div class="col-xl-2"></div>
                            <div class="col-xl-8">
                                <div class="kt-section kt-section--first">
                                    <div class="kt-section__body">
                                        <h3 class="kt-section__title kt-section__title-lg">Pharmacy Demographic:</h3>
                                        <div class="form-group row">
                                            <label class="col-lg-2 col-form-label">NCPDP:</label>
                                            <div class="col-lg-3">
                                                <input asp-for="Pharmacy.NCPDP" class="form-control" disabled />
                                            </div>
                                            <label class="col-lg-2 col-form-label">NPI:</label>
                                            <div class="col-lg-3">
                                                <input asp-for="Pharmacy.NPI" class="form-control" disabled />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-lg-2 col-form-label">Pharmacy Name:</label>
                                            <div class="col-lg-3">
                                                <input asp-for="Pharmacy.PharmacyName" class="form-control" disabled />
                                            </div>
                                            <label class="col-lg-2 col-form-label">Address:</label>
                                            <div class="col-lg-3">
                                                <input asp-for="Pharmacy.Address" class="form-control" disabled />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-lg-2 col-form-label">City:</label>
                                            <div class="col-lg-3">
                                                <input asp-for="Pharmacy.CityName" class="form-control" disabled />
                                            </div>
                                            <label class="col-lg-2 col-form-label">County:</label>
                                            <div class="col-lg-3">
                                                <input asp-for="Pharmacy.County" class="form-control" disabled />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-lg-2 col-form-label">State:</label>
                                            <div class="col-lg-3">
                                                <input asp-for="Pharmacy.StateCode" class="form-control" disabled />
                                            </div>
                                            <label class="col-lg-2 col-form-label">Zip Code:</label>
                                            <div class="col-lg-3">
                                                <input asp-for="Pharmacy.ZipCode" class="form-control" disabled />
                                            </div>
                                        </div>
                                        <div class="form-group row form-group-last">
                                            <label class="col-lg-2 col-form-label">Phone:</label>
                                            <div class="col-lg-3">
                                                <input asp-for="Pharmacy.Phone" class="form-control" disabled />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="kt-separator kt-separator--border-dashed kt-separator--space-lg"></div>
                                <div class="kt-section kt-section--last">
                                    <h3 class="kt-section__title kt-section__title-lg">Pharmacy Status:</h3>
                                    <div class="form-group row">
                                        <div class="col-lg-1"></div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold @(Model.ActionType == ActionType.Edit ? null : "kt-checkbox--disabled")">
                                                    <input type="checkbox" asp-for="PharmacyStatus.PreferredFlag" checked="@(Model.PharmacyStatus.PreferredFlag == "true" ? "checked" : null)" disabled="@(Model.ActionType == ActionType.Edit && Model.User.RoleDesc != "Provider_Group" ? null : "disabled")" />
                                                    Preferred
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold @(Model.ActionType == ActionType.Edit ? null : "kt-checkbox--disabled")">
                                                    <input type="checkbox" asp-for="PharmacyStatus.AdherenceFlag" checked="@(Model.PharmacyStatus.AdherenceFlag == "true" ? "checked" : null)" disabled="@(Model.ActionType == ActionType.Edit && Model.User.RoleDesc != "Provider_Group" ? null : "disabled")" />
                                                    Adherence
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold @(Model.ActionType == ActionType.Edit ? null : "kt-checkbox--disabled")">
                                                    <input type="checkbox" asp-for="PharmacyStatus.DirectoryDisplayFlag" checked="@(Model.PharmacyStatus.DirectoryDisplayFlag == "true" ? "checked" : null)" disabled="@(Model.ActionType == ActionType.Edit ? null : "disabled")" />
                                                    Directory
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-1"></div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-lg-1"></div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.LtcFlag" checked="@(Model.PharmacyStatus.LtcFlag == "true" ? "checked" : null)" disabled />
                                                    LTC
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.IhsFlag" checked="@(Model.PharmacyStatus.IhsFlag == "true" ? "checked" : null)" disabled />
                                                    IHS
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.MailFlag" checked="@(Model.PharmacyStatus.MailFlag == "true" ? "checked" : null)" disabled />
                                                    Mail
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-1"></div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-lg-1"></div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.MailPreferredFlag" checked="@(Model.PharmacyStatus.MailPreferredFlag == "true" ? "checked" : null)" disabled />
                                                    Mail Preferred
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.RetailFlag" checked="@(Model.PharmacyStatus.RetailFlag == "true" ? "checked" : null)" disabled />
                                                    Retail
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.LimitedAccessFlag" checked="@(Model.PharmacyStatus.LimitedAccessFlag == "true" ? "checked" : null)" disabled />
                                                    Limited Access
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-1"></div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-lg-1"></div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.eRxFlag" checked="@(Model.PharmacyStatus.eRxFlag == "true" ? "checked" : null)" disabled />
                                                    ERx
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.TtyFlag" checked="@(Model.PharmacyStatus.TtyFlag == "true" ? "checked" : null)" disabled />
                                                    TTY
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.Hours24Flag" checked="@(Model.PharmacyStatus.Hours24Flag == "true" ? "checked" : null)" disabled />
                                                    24 Hours
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-1"></div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-lg-1"></div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.ContractedFlag" checked="@(Model.PharmacyStatus.ContractedFlag == "true" ? "checked" : null)" disabled />
                                                    Contracted
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.PartBFlag" checked="@(Model.PharmacyStatus.PartBFlag == "true" ? "checked" : null)" disabled />
                                                    Part B
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.SpecialtyFlag" checked="@(Model.PharmacyStatus.SpecialtyFlag == "true" ? "checked" : null)" disabled />
                                                    Specialty
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-1"></div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-lg-1"></div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.RetailExtSupplyFlag" checked="@(Model.PharmacyStatus.RetailExtSupplyFlag == "true" ? "checked" : null)" disabled />
                                                    Retail Ext Supply
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="kt-checkbox-single">
                                                <label class="kt-checkbox kt-checkbox--bold kt-checkbox--disabled">
                                                    <input type="checkbox" asp-for="PharmacyStatus.HifFlag" checked="@(Model.PharmacyStatus.HifFlag == "true" ? "checked" : null)" disabled />
                                                    HIF
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                        </div>
                                        <div class="col-lg-1"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-2"></div>
                        </div>
                    </div>
                </div>
            </div>
            <!--end::Portlet-->
        </div>
    </div>
</form>
